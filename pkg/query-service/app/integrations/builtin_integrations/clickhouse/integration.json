  "data_collected": {
    "logs": [
      {
        "name": "Process ID",
        "path": "attributes.pid",
        "type": "string"
      },
      {
        "name": "Timestamp",
        "path": "timestamp",
        "type": "timestamp"
      },
      {
        "name": "Severity Text",
        "path": "severity_text",
        "type": "string"
      },
      {
        "name": "Severity Number",
        "path": "severity_number",
        "type": "number"
      }
    ],
    "metrics": [
      {
        "name": "clickhouse_backends",
        "type": "sum",
        "unit": "number",
        "description": "The number of backends."
      }
